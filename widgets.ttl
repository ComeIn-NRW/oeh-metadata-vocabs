    skos:definition "Neuigkeiten aus Forschung und Politik zum Fach."@de ;
    skos:broader <good_to_know> ;
    skos:inScheme <> .

<intercultural> a skos:Concept ;
    skos:prefLabel "Interkulturelles"@de ;
