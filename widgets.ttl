    skos:definition "Landkarte mit und ohne Interaktivität."@de ;
    skos:broader <materialien> ;
    skos:inScheme <> .

<photograph> a skos:Concept ;
    skos:prefLabel "Foto"@de ;
    skos:broader <materialien> ;
    skos:inScheme <> .

<vocab_list> a skos:Concept ;
