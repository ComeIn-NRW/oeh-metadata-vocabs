    skos:inScheme <> .

<podcast_radio> a skos:Concept ;
    skos:prefLabel "Radio, Podcast"@de ;
    skos:broader <materialien> ;
    skos:inScheme <> .

<augmented_reality> a skos:Concept ;
    skos:prefLabel "Augmented Reality / Virtual Reality"@de ;
    skos:broader <materialien> ;
    skos:inScheme <> .

<graphic> a skos:Concept ;
